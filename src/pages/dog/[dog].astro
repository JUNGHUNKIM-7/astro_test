---
import Layout from '../../components/astro/Layout.astro'

export function getStaticPaths() {
    return [
        { params: { dog: 'clifford' } },
        { params: { dog: 'rover' } },
        { params: { dog: 'spot' } },
    ]
}

const { dog } = Astro.params
---

<Layout>
    <div>Good dog, {dog}!</div>
</Layout>
